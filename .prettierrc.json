{
  "semi": true,
  "singleQuote": false,
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "trailingComma": "es5",
  "tailwindConfig": "./tailwind.config.ts",
  "tabWidth": 2,
  "printWidth": 80,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "tailwindFunctions": ["clsx", "cn", "twMerge"]
}
